{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        },
        {
          "key": "Content-Security-Policy",
          "value": "default-src 'self' *.croomssched.tech; img-src *; media-src *; script-src *.croomssched.tech"
        }
      ]
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/teacher",
      "status": 200,
      "dest": "/error/200.html"
    },
    {
      "src": "/derp",
      "status": 418,
      "dest": "/error/418.html"
    },
    {
      "src": "/bob",
      "status": 516,
      "dest": "/error/516.html"
    },
    {
      "src": "/(.*)",
      "status": 404,
      "dest": "/error/404.html"
    }
  ]
}