{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        },
        {
          "key": "Content-Security-Policy",
          "value": "default-src 'self' *.croomssched.tech; img-src *; media-src *; script-src *.croomssched.tech"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/teacher",
      "destination": "/error/200.html"
    },
    {
      "source": "/derp",
      "destination": "/error/418.html"
    },
    {
      "source": "/bob",
      "destination": "/error/516.html"
    },
    {
      "source": "/(.*)",
      "destination": "/error/404.html"
    }
  ]
}